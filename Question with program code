Q1. Write a program to print unit digit of a given number
#include<stdio.h>
int main()
{
     int x;
     printf("Enter a number");
     scanf("%d",&x);
     printf("Unit digit of %d is %d",x,x%10);
     printf("\n");
     return 0;
}

Q2 Write a program to print a given number without its last digit.
#include<stdio.h>
int main()
{
     int x;
     printf("Enter a number");
     scanf("%d",&x);
     printf("Number without last digit is %d",x/10);
     printf("\n");
     return 0;
}

Q3 Write a program to swap values of two int variables
#include<stdio.h>
int main()
{
     int a,b,c;
     printf("Enter two number");
     scanf("%d%d",&a,&b);
     printf("a=%d b=%d",a,b);
     c=a;
     a=b;
     b=c;
     printf("\na=%d b=%d",a,b);
     printf("\n");
     return 0;
}

Q4 Write a program to swap values of two int variables without using a third variable.
#include<stdio.h>
int main()
{
     int a,b;
     printf("Enter two number");
     scanf("%d%d",&a,&b);
     printf("a=%d b=%d",a,b);
     a=a+b;
     b=a-b;
     a=a-b;
     printf("\na=%d b=%d",a,b);
     printf("\n");
     return 0;
}

Q5 Write a program to input a three-digit number and display the sum of the digits.
#include<stdio.h>
int main()
{
 int x=126;
 int rem=0,sum=0;

 rem =x%10;
 x =x/10;
 sum =sum+rem;

 rem =x%10;
 x =x/10;
 sum =sum+rem;

 rem =x%10;
 x =x/10;
 sum =sum+rem;

printf("%d",sum);
    return 0;
}

Q6 Write a program which takes a character as an input and displays its ASCII code.
#include<stdio.h>
int main()
{
    char c;
    printf("Enter character:");
    scanf("%c",&c);
    printf("character ASCII code is %d",c);
    printf("\n");
    return 0;
}

